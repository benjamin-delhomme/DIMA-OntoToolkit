##########  ACT tactics  #######################################

dima:IndividualValorisation
    a owl:Class , owl:NamedIndividual , dima:Tactic , dima:ActTactic ;
    rdfs:subClassOf dima:ActTactic ;
    rdfs:label "Individual valorisation"@en , "Valorisation individuelle"@fr ;
    dima:valueCode "TA0041" ;
    rdfs:comment "A tactic that emphasizes personal judgment and self-confidence."@en ,
                 "Tactique qui valorise le jugement personnel et la confiance en soi."@fr ;
    dima:hasTechnique dima:OverconfidenceBias ,
                      dima:PeltzmanEffect ,
                      dima:IllusorySuperiority .

dima:EscalationReinforcement
    a owl:Class , owl:NamedIndividual , dima:Tactic , dima:ActTactic ;
    rdfs:subClassOf dima:ActTactic ;
    rdfs:label "Escalation reinforcement"@en , "Renforcement escalatoire"@fr ;
    dima:valueCode "TA0042" ;
    rdfs:comment "A tactic that encourages continuation of actions already initiated."@en ,
                 "Tactique qui favorise la réalisation d'actions déjà engagées."@fr ;
    dima:hasTechnique dima:SunkCostBias ,
                      dima:AuthorityBias .

dima:OzaekomiWaza
    a owl:Class , owl:NamedIndividual , dima:Tactic , dima:ActTactic ;
    rdfs:subClassOf dima:ActTactic ;
    rdfs:label "Ozaekomi waza (immobilization control)"@en ,
               "Ozaekomi waza (contrôle par immobilisation)"@fr ;
    dima:valueCode "TA0043" ;
    rdfs:comment "A tactic that induces passivity or decision paralysis in the target."@en ,
                 "Tactique qui consiste à favoriser chez une cible, l'immobilisme et l'absence d'action."@fr ;
    dima:hasTechnique dima:OmissionBias ,
                      dima:StatusQuoBias ,
                      dima:InformationOverload .

##########  ACT techniques  ####################################

dima:OverconfidenceBias
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:ActTechnique ;
    rdfs:subClassOf dima:ActTechnique ;
    rdfs:label "overconfidence bias"@en , "biais d'excès de confiance"@fr ;
    dima:valueCode "TE0411" ;
    rdfs:comment "Overestimating one's abilities, performance, or judgment."@en ,
                 "Surestimer ses compétences, sa performance ou son jugement."@fr .

dima:PeltzmanEffect
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:ActTechnique ;
    rdfs:subClassOf dima:ActTechnique ;
    rdfs:label "Peltzman effect"@en , "Effet Peltzman (compensation du risque)"@fr ;
    dima:valueCode "TE0412" ;
    rdfs:comment "Tendency to take greater risks when one feels protected."@en ,
                 "Tendance à prendre plus de risques en se sentant protégé."@fr .

dima:IllusorySuperiority
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:ActTechnique ;
    rdfs:subClassOf dima:ActTechnique ;
    rdfs:label "illusory superiority"@en , "Supériorité illusoire"@fr ;
    dima:valueCode "TE0413" ;
    rdfs:comment "Belief that one is better than others, often unrealistically."@en ,
                 "Croyance que l’on est meilleur que les autres, souvent à tort."@fr .

dima:SunkCostBias
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:ActTechnique ;
    rdfs:subClassOf dima:ActTechnique ;
    rdfs:label "sunk cost bias"@en , "biais des coûts irrécupérables"@fr ;
    dima:valueCode "TE0421" ;
    rdfs:comment "Tendency to continue an endeavor once an investment in money, effort, or time has been made."@en ,
                 "Tendance à poursuivre un effort déjà engagé en raison d’un investissement passé."@fr .

dima:AuthorityBias
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:ActTechnique ;
    rdfs:subClassOf dima:ActTechnique ;
    rdfs:label "authority bias"@en , "biais d'autorité"@fr ;
    dima:valueCode "TE0422" ;
    rdfs:comment "Tendency to follow directives or believe information from perceived authorities."@en ,
                 "Tendance à accorder plus de crédibilité ou à suivre les directives de figures d’autorité."@fr .

dima:OmissionBias
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:ActTechnique ;
    rdfs:subClassOf dima:ActTechnique ;
    rdfs:label "omission bias"@en , "Biais d'omission"@fr ;
    dima:valueCode "TE0431" ;
    rdfs:comment "Preferring inaction over action even when it leads to worse outcomes."@en ,
                 "Considérer qu'un tort causé par inaction est moins grave qu’un tort causé par action."@fr .

dima:StatusQuoBias
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:ActTechnique ;
    rdfs:subClassOf dima:ActTechnique ;
    rdfs:label "status quo bias"@en , "Biais du statu quo"@fr ;
    dima:valueCode "TE0432" ;
    rdfs:comment "Preference for keeping things as they are, avoiding change."@en ,
                 "Préférence pour le statu quo même si d'autres options sont meilleures."@fr .

dima:InformationOverload
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:ActTechnique ;
    rdfs:subClassOf dima:ActTechnique ;
    rdfs:label "information overload"@en , "Saturation informationnelle"@fr ;
    dima:valueCode "TE0433" ;
    rdfs:comment "Overloading a target with information to disrupt decision-making (analysis paralysis)."@en ,
                 "Saturation de la cible d’informations afin de paralyser la prise de décision."@fr .
