##########  INFORM tactics  ####################################

dima:PatternCreation
    a owl:Class , owl:NamedIndividual , dima:Tactic , dima:InformTactic ;
    rdfs:subClassOf dima:InformTactic ;
    rdfs:label "Pattern creation"@en , "Création d’un motif"@fr ;
    dima:valueCode "TA0021" ;
    rdfs:comment "A tactic that integrates received information into an existing narrative."@en ,
                 "Tactique qui permet de mettre en récit une information reçue, de l’intégrer dans un contexte préexistant."@fr ;
    dima:hasTechnique dima:IllusoryCorrelationBias ,
                      dima:AnecdotalEvidenceBias ,
                      dima:GamblersFallacy .

dima:GeneralizationAndStereotypes
    a owl:Class , owl:NamedIndividual , dima:Tactic , dima:InformTactic ;
    rdfs:subClassOf dima:InformTactic ;
    rdfs:label "Generalization and reinforcement of stereotypes"@en , "Généralisation et renforcement de stéréotypes"@fr ;
    dima:valueCode "TA0022" ;
    rdfs:comment "A tactic aiming to simplify perceived information to make it more acceptable or understandable to the target."@en ,
                 "Tactique qui vise à simplifier une ou des informations perçues pour les rendre acceptables et/ou intelligibles par la cible."@fr ;
    dima:hasTechnique dima:BackfireEffect .

dima:FamiliaritySuperiority
    a owl:Class , owl:NamedIndividual , dima:Tactic , dima:InformTactic ;
    rdfs:subClassOf dima:InformTactic ;
    rdfs:label "Familiarity superiority"@en , "Supériorité familière"@fr ;
    dima:valueCode "TA0023" ;
    rdfs:comment "A tactic that enhances the perceived value of information through familiarity or proximity."@en ,
                 "Tactique qui permet de valoriser une information aux yeux de la cible par exploitation des proximités."@fr ;
    dima:hasTechnique dima:HomogeneityBias ,
                      dima:WellTraveledRoadBias .

dima:Simplification
    a owl:Class , owl:NamedIndividual , dima:Tactic , dima:InformTactic ;
    rdfs:subClassOf dima:InformTactic ;
    rdfs:label "Simplification"@en , "Simplification"@fr ;
    dima:valueCode "TA0024" ;
    rdfs:comment "A tactic that simplifies concepts and emphasizes scale to improve information acceptability."@en ,
                 "Tactique qui simplifie les concepts et favorise les ordres de grandeurs pour faciliter l'acceptabilité d'une information."@fr ;
    dima:hasTechnique dima:ZeroSumBias ,
                      dima:NormalcyBias .

dima:SelfReference
    a owl:Class , owl:NamedIndividual , dima:Tactic , dima:InformTactic ;
    rdfs:subClassOf dima:InformTactic ;
    rdfs:label "Self-reference"@en , "Auto-référence"@fr ;
    dima:valueCode "TA0025" ;
    rdfs:comment "A tactic based on the assumption that the audience shares the same knowledge and references."@en ,
                 "Tactique qui exploite le fait que la cible pense que ses interlocuteurs partagent ses références communes."@fr ;
    dima:hasTechnique dima:FalseConsensusEffect .

dima:TemporalProjection
    a owl:Class , owl:NamedIndividual , dima:Tactic , dima:InformTactic ;
    rdfs:subClassOf dima:InformTactic ;
    rdfs:label "Temporal projection"@en , "Projection temporelle"@fr ;
    dima:valueCode "TA0026" ;
    rdfs:comment "A tactic that draws on (real or fabricated) memories to legitimize a message."@en ,
                 "Tactique qui s’appuie sur les souvenirs (réels ou forgés) pour crédibiliser un discours ou une information."@fr ;
    dima:hasTechnique dima:HindsightBias .

##########  INFORM techniques  #################################

dima:IllusoryCorrelationBias
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:InformTechnique ;
    rdfs:subClassOf dima:InformTechnique ;
    rdfs:label "illusory correlation bias"@en , "Biais de corrélation illusoire"@fr ;
    dima:valueCode "TE0211" ;
    rdfs:comment "Perceiving a false link between unrelated events or data."@en ,
                 "Tendance à percevoir un lien entre des événements ou des éléments non corrélés."@fr .

dima:AnecdotalEvidenceBias
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:InformTechnique ;
    rdfs:subClassOf dima:InformTechnique ;
    rdfs:label "anecdotal evidence bias"@en , "Biais de la preuve anecdotique"@fr ;
    dima:valueCode "TE0212" ;
    rdfs:comment "Giving more weight to vivid personal stories than to statistical evidence."@en ,
                 "Accorder plus de poids aux récits personnels qu'aux preuves statistiques."@fr .

dima:GamblersFallacy
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:InformTechnique ;
    rdfs:subClassOf dima:InformTechnique ;
    rdfs:label "gambler’s fallacy"@en , "Illusion des séries"@fr ;
    dima:valueCode "TE0213" ;
    rdfs:comment "Believing that past random events influence future ones."@en ,
                 "Croire que les événements passés influencent la probabilité des événements futurs."@fr .

dima:BackfireEffect
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:InformTechnique ;
    rdfs:subClassOf dima:InformTechnique ;
    rdfs:label "backfire effect"@en , "Effet de retournement"@fr ;
    dima:valueCode "TE0221" ;
    rdfs:comment "Reinforcing one’s beliefs when confronted with contradictory information."@en ,
                 "Tendance à renforcer ses croyances lorsqu'on est confronté à des preuves contraires."@fr .

dima:HomogeneityBias
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:InformTechnique ;
    rdfs:subClassOf dima:InformTechnique ;
    rdfs:label "homogeneity bias"@en , "Biais d'homogénéité"@fr ;
    dima:valueCode "TE0231" ;
    rdfs:comment "Perceiving members of an out-group as more similar than they really are."@en ,
                 "Tendance à voir les membres d’un groupe externe comme homogènes."@fr .

dima:WellTraveledRoadBias
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:InformTechnique ;
    rdfs:subClassOf dima:InformTechnique ;
    rdfs:label "well-traveled road bias"@en , "Biais de la route connue"@fr ;
    dima:valueCode "TE0232" ;
    rdfs:comment "Preferring familiar paths even when others are better."@en ,
                 "Préférer ce qui est familier, même si d'autres options sont meilleures."@fr .

dima:ZeroSumBias
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:InformTechnique ;
    rdfs:subClassOf dima:InformTechnique ;
    rdfs:label "zero-sum bias"@en , "Biais de la somme nulle"@fr ;
    dima:valueCode "TE0241" ;
    rdfs:comment "Believing that gains for others mean losses for oneself."@en ,
                 "Croire que le gain de l’un implique la perte de l’autre."@fr .

dima:NormalcyBias
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:InformTechnique ;
    rdfs:subClassOf dima:InformTechnique ;
    rdfs:label "normalcy bias"@en , "Biais de normalité"@fr ;
    dima:valueCode "TE0242" ;
    rdfs:comment "Underestimating the likelihood of a crisis because things have always been normal."@en ,
                 "Penser que rien ne changera parce que les choses ont toujours été normales."@fr .

dima:FalseConsensusEffect
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:InformTechnique ;
    rdfs:subClassOf dima:InformTechnique ;
    rdfs:label "false consensus effect"@en , "Effet de faux consensus"@fr ;
    dima:valueCode "TE0251" ;
    rdfs:comment "Overestimating how much others share your views or behaviors."@en ,
                 "Surestimer à quel point les autres partagent nos opinions ou comportements."@fr .

dima:HindsightBias
    a owl:Class , owl:NamedIndividual , dima:Technique , dima:InformTechnique ;
    rdfs:subClassOf dima:InformTechnique ;
    rdfs:label "hindsight bias"@en , "Biais rétrospectif"@fr ;
    dima:valueCode "TE0261" ;
    rdfs:comment "Believing events were more predictable after they happened."@en ,
                 "Croire qu’un événement était prévisible après qu’il se soit produit."@fr .

