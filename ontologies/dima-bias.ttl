### MEMORISE Phase #####
########################

dima:MemoriseTactic a owl:Class ;
    rdfs:subClassOf dima:Tactic ,
        [ a owl:Restriction ;
          owl:onProperty dima:isTacticOf ;
          owl:someValuesFrom dima:Memorise ] ;
    rdfs:label "Memorise tactic"@en ;
    rdfs:comment "Tactics associated with the Memorise phase."@en .

dima:MemoriseTechnique  a owl:Class ;
    rdfs:subClassOf
        dima:Technique ,
        [ a owl:Restriction ;
          owl:onProperty     dima:isTechniqueOf ;
          owl:someValuesFrom dima:MemoriseTactic ] ;
    rdfs:label   "Memorise technique"@en ;
    rdfs:comment "Techniques that belong to Memorise‑phase tactics."@en .

