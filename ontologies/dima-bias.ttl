        [ a owl:Restriction ;
          owl:onProperty     dima:isTechniqueOf ;
          owl:someValuesFrom dima:ActTactic ] ;
    rdfs:label   "Act technique"@en ;
    rdfs:comment "Techniques that belong to Act‑phase tactics."@en .

