############################
### Annotation Properties ##
############################

dima:uuid a owl:AnnotationProperty ;
    rdfs:label "UUID"@en ;
    rdfs:label "UUID"@fr ;
    rdfs:comment "Stable unique identifier from the original taxonomy source."@en ;
    rdfs:comment "Identifiant unique et stable issu de la taxonomie source."@fr .

dima:valueCode a owl:AnnotationProperty ;
    rdfs:label "Value code"@en ;
    rdfs:label "Code de valeur"@fr ;
    rdfs:comment "Code from the source taxonomy (e.g., TA0011, TE0111)."@en ;
    rdfs:comment "Code de la taxonomie source (ex. : TA0011, TE0111)."@fr .


#####################
### DETECT Phase  ###
#####################

dima:DetectTactic a owl:Class ;
    rdfs:subClassOf dima:Tactic ,
        [ a owl:Restriction ;
          owl:onProperty dima:isTacticOf ;
          owl:someValuesFrom dima:Detect ] ;
    rdfs:label "Detect tactic"@en ;
    rdfs:comment "Tactics associated with the Detect phase."@en .

dima:DetectTechnique  a owl:Class ;
    rdfs:subClassOf
        dima:Technique ,
        [ a owl:Restriction ;
          owl:onProperty     dima:isTechniqueOf ;
          owl:someValuesFrom dima:DetectTactic ] ;
    rdfs:label   "Detect technique"@en ;
    rdfs:comment "Techniques that belong to Detect‑phase tactics."@en .

