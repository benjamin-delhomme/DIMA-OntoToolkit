@prefix scim: <https://stratcomcoe.org/influence-mini/ontology#> .
@prefix dima: <https://m82-project.org/dima-bias/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<https://m82-project.org/dima-bias/ontology> a owl:Ontology ;
    rdfs:label "DIMA Bias Ontology – Unified"@en ;
    rdfs:label "Ontologie des biais DIMA – Unifiée"@fr ;
    owl:versionIRI <https://m82-project.org/dima-bias/ontology/v1.0.0> ;
    dcterms:source <https://github.com/M82-project/DIMA/tree/bf4ae6797d799fe7f0cd56e4263a209ba66c1e0c> ;
    dcterms:hasVersion "bf4ae6797d799fe7f0cd56e4263a209ba66c1e0c" ;
    owl:imports <https://stratcomcoe.org/influence-mini/ontology> .

#######################
### Core Ontology #####
#######################

dima:Phase a owl:Class ;
    rdfs:label "DIMA Phase"@en ;
    rdfs:label "Phase DIMA"@fr ;
    rdfs:comment "One of the four major stages in the DIMA framework: Detect, Inform, Memorize, Act."@en ;
    rdfs:comment "L'une des quatre phases majeures du cadre DIMA : Détecter, Informer, Mémoriser, Agir."@fr .

dima:Tactic a owl:Class ;
    rdfs:label "Narrative Tactic"@en ;
    rdfs:label "Tactique narrative"@fr ;
    rdfs:comment "A communicative strategy used to exploit cognitive biases in narrative content."@en ;
    rdfs:comment "Une stratégie narrative exploitant des biais cognitifs dans un discours."@fr .

dima:Technique a owl:Class ;
    rdfs:label "Cognitive Technique"@en ;
    rdfs:label "Technique cognitive"@fr ;
    rdfs:comment "A cognitive bias or psychological mechanism leveraged by a tactic."@en ;
    rdfs:comment "Un biais cognitif ou mécanisme psychologique utilisé dans une tactique."@fr .

dima:TechniqueUsage   rdf:type  owl:Class ;
                      rdfs:label "Technique Usage"@en ;
                      rdfs:comment "A concrete occurrence of a technique inside an argument."@en .

dima:Detect
    a owl:Class , owl:NamedIndividual , dima:Phase ;
    rdfs:subClassOf dima:Phase ;
    rdfs:label "DETECT"@en , "DÉTECTER"@fr ;
    dima:hasTactic
        dima:PreExistingInformation ,
        dima:RepeatedExposureInformation ,
        dima:DivisiveInformation ,
        dima:DeviationFromNorm ,
        dima:SignificantDetail .

dima:Inform
    a owl:Class , owl:NamedIndividual , dima:Phase ;
    rdfs:subClassOf dima:Phase ;
    rdfs:label "INFORM"@en , "INFORMER"@fr ;
    dima:hasTactic
        dima:PatternCreation ,
        dima:GeneralizationAndStereotypes ,
        dima:FamiliaritySuperiority ,
        dima:Simplification ,
        dima:SelfReference ,
        dima:TemporalProjection .

dima:Memorise
    a owl:Class , owl:NamedIndividual , dima:Phase ;
    rdfs:subClassOf dima:Phase ;
    rdfs:label "MEMORISE"@en , "MÉMORISER"@fr ;
    dima:hasTactic
        dima:IndirectReinforcement ,
        dima:PreexistingReinforcement ,
        dima:ContentExposure .

dima:Act
    a owl:Class , owl:NamedIndividual , dima:Phase ;
    rdfs:subClassOf dima:Phase ;
    rdfs:label "ACT"@en , "AGIR"@fr ;
    dima:hasTactic
        dima:IndividualValorisation ,
        dima:EscalationReinforcement ,
        dima:OzaekomiWaza .

dima:Tactic owl:disjointWith dima:Technique .
dima:Phase owl:disjointWith dima:Tactic .
dima:Phase owl:disjointWith dima:Technique .

##########################
### Data Properties ####
##########################

dima:hasExplanation  rdf:type  owl:DatatypeProperty ;
    rdfs:label   "has explanation"@en ;
    rdfs:comment "Natural‑language explanation attached to a TechniqueUsage node."@en ;
    rdfs:domain  dima:TechniqueUsage ;
    rdfs:range   xsd:string .

##########################
### Object Properties ####
##########################


dima:usesTechnique a owl:ObjectProperty ;
    rdfs:label "uses technique"@en ;
    rdfs:label "utilise une technique"@fr ;
    rdfs:comment "Links an argument, premise, development, or conclusion to a TechniqueUsage node that represents a concrete use of a technique."@en ;
    rdfs:comment "Associe un argument, une prémisse, un développement ou une conclusion à un nœud TechniqueUsage qui représente un usage concret d’une technique."@fr ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf (
            scim:Argument
            scim:Premise
            scim:Development
            scim:Conclusion
        )
    ] ;
    rdfs:range dima:TechniqueUsage ;
    owl:inverseOf dima:isTechniqueUsedBy .
	
dima:isTechniqueUsedBy a owl:ObjectProperty ;
    rdfs:label "is used in argument"@en ;
    rdfs:label "est utilisée dans un argument"@fr ;
    rdfs:comment "Inverse property linking a TechniqueUsage node to the argument that contains it."@en ;
    rdfs:comment "Propriété inverse reliant un nœud TechniqueUsage à l’argument qui le contient."@fr ;
    rdfs:domain dima:TechniqueUsage ;
    rdfs:range [
        a owl:Class ;
        owl:unionOf (
            scim:Argument
            scim:Premise
            scim:Development
            scim:Conclusion
        )
    ] .

dima:instantiatesTechnique a owl:ObjectProperty ;
    rdfs:label "instantiates technique"@en ;
    rdfs:label "instancie la technique"@fr ;
    rdfs:comment "Connects a TechniqueUsage node to the abstract cognitive technique it instantiates."@en ;
    rdfs:comment "Relie un nœud TechniqueUsage à la technique cognitive qu’il instancie."@fr ;
    rdfs:domain dima:TechniqueUsage ;
    rdfs:range dima:Technique ;
    owl:inverseOf dima:isInstantiatedByTechniqueUsage .

dima:isInstantiatedByTechniqueUsage a owl:ObjectProperty ;
    rdfs:label "has concrete usage"@en ;
    rdfs:label "a un usage concret"@fr ;
    rdfs:comment "Inverse linking a technique to its concrete TechniqueUsage nodes."@en ;
    rdfs:comment "Propriété inverse reliant une technique à ses nœuds TechniqueUsage concrets."@fr ;
    rdfs:domain dima:Technique ;
    rdfs:range dima:TechniqueUsage .

dima:hasTactic a owl:ObjectProperty ;
    rdfs:label "has tactic"@en ;
    rdfs:label "possède une tactique"@fr ;
    rdfs:domain dima:Phase ;
    rdfs:range dima:Tactic ;
    rdfs:comment "Associates a narrative tactic with a DIMA phase (Detect, Inform, etc.)."@en ;
    rdfs:comment "Associe une tactique narrative à une phase DIMA (Détecter, Informer, etc.)."@fr .

dima:isTacticOf a owl:ObjectProperty ;
    owl:inverseOf dima:hasTactic ;
    rdfs:label "is tactic of"@en ;
    rdfs:label "est une tactique de"@fr ;
    rdfs:domain dima:Tactic ;
    rdfs:range dima:Phase ;
    rdfs:comment "Inverse property linking a tactic to its corresponding DIMA phase."@en ;
    rdfs:comment "Propriété inverse associant une tactique à sa phase DIMA correspondante."@fr .

dima:hasTechnique a owl:ObjectProperty ;
    rdfs:label "has technique"@en ;
    rdfs:label "possède une technique"@fr ;
    rdfs:domain dima:Tactic ;
    rdfs:range dima:Technique ;
    rdfs:comment "Links a narrative tactic to one or more cognitive techniques."@en ;
    rdfs:comment "Relie une tactique narrative à une ou plusieurs techniques cognitives."@fr .

dima:isTechniqueOf a owl:ObjectProperty ;
    owl:inverseOf dima:hasTechnique ;
    rdfs:label "is technique of"@en ;
    rdfs:label "est une technique de"@fr ;
    rdfs:domain dima:Technique ;
    rdfs:range dima:Tactic ;
    rdfs:comment "Inverse property linking a technique to its associated tactic(s)."@en ;
    rdfs:comment "Propriété inverse reliant une technique aux tactiques auxquelles elle est associée."@fr .
	
