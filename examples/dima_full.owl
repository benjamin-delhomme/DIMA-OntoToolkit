    <rdf:type rdf:resource="https://m82-project.org/dima-bias/ontology#TechniqueUsage"/>
    <ns1:hasExplanation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Both premises frame the situation in negative terms (disruption, tension) without neutral or positive balance, emphasizing threat and instability to exploit negativity bias.</ns1:hasExplanation>
  </rdf:Description>
  <rdf:Description rdf:about="https://m82-project.org/dima-bias/ontology#c9ce7d9bb7_omissionbias_3">
    <ns1:instantiatesTechnique rdf:resource="https://m82-project.org/dima-bias/ontology#OmissionBias"/>
