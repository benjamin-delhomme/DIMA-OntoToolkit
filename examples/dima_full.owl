  <rdf:Description rdf:about="https://m82-project.org/dima-bias/ontology#ImplicitStereotype">
    <rdf:type rdf:resource="https://m82-project.org/dima-bias/ontology#MemoriseTechnique"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="https://m82-project.org/dima-bias/ontology#Technique"/>
    <rdfs:label xml:lang="fr">Stéréotype implicite</rdfs:label>
    <rdfs:label xml:lang="en">implicit stereotype</rdfs:label>
