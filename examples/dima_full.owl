  <rdf:Description rdf:about="https://m82-project.org/dima-bias/ontology#c9ce7d9bb7_omissionbias_2">
    <ns1:hasExplanation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The argument frames Caldria's inaction as morally superior and praiseworthy despite ongoing harm, implicitly suggesting that acting (defending its interests) would be less acceptable than doing nothing.</ns1:hasExplanation>
    <rdf:type rdf:resource="https://m82-project.org/dima-bias/ontology#TechniqueUsage"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
