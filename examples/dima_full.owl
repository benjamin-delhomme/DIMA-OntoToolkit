  <rdf:Description rdf:about="https://m82-project.org/dima-bias/ontology#c9ce7d9bb7_vonrestorffeffect_1">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="https://m82-project.org/dima-bias/ontology#TechniqueUsage"/>
    <ns1:hasExplanation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The argument isolates the warning from 'independent experts' as a distinct and memorable element, contrasting it with the official Velmoran stance and drawing special attention to this perspective.</ns1:hasExplanation>
    <ns1:instantiatesTechnique rdf:resource="https://m82-project.org/dima-bias/ontology#VonRestorffEffect"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://m82-project.org/dima-bias/ontology#c9ce7d9bb7_vonrestorffeffect_3">
    <ns1:hasExplanation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The use of direct quotations from a named individual, set apart from the general narrative, isolates these statements and makes them stand out, exploiting the Von Restorff Effect to make the support and defiance more memorable.</ns1:hasExplanation>
    <rdf:type rdf:resource="https://m82-project.org/dima-bias/ontology#TechniqueUsage"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <ns1:instantiatesTechnique rdf:resource="https://m82-project.org/dima-bias/ontology#VonRestorffEffect"/>
  </rdf:Description>
