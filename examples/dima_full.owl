    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">availability heuristic</rdfs:label>
    <rdfs:label xml:lang="fr">Heuristique de disponibilité</rdfs:label>
    <ns1:valueCode rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TE0111</ns1:valueCode>
  </rdf:Description>
  <rdf:Description rdf:about="https://m82-project.org/dima-bias/ontology#AnecdotalEvidenceBias">
