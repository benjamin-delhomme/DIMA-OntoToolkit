  <rdf:Description rdf:about="https://m82-project.org/dima-bias/ontology#c9ce7d9bb7_vonrestorffeffect_4">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="https://m82-project.org/dima-bias/ontology#TechniqueUsage"/>
    <ns1:instantiatesTechnique rdf:resource="https://m82-project.org/dima-bias/ontology#VonRestorffEffect"/>
    <ns1:hasExplanation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The development isolates the action of the 'international community' as a distinct, memorable shift, setting it apart from the more routine actions of regional powers and the Security Council, thus exploiting the Von Restorff Effect.</ns1:hasExplanation>
  </rdf:Description>
