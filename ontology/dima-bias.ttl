@prefix dima: <https://m82-project.org/dima/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://m82-project.org/dima/ontology> a owl:Ontology ;
    rdfs:label "DIMA Bias Ontology – Unified"@en ;
    rdfs:label "Ontologie des biais DIMA – Unifiée"@fr ;
    owl:versionIRI <https://m82-project.org/dima/ontology/v1.0.0> ;
    dcterms:source <https://github.com/M82-project/DIMA/tree/bf4ae6797d799fe7f0cd56e4263a209ba66c1e0c> ;
    dcterms:hasVersion "bf4ae6797d799fe7f0cd56e4263a209ba66c1e0c" .

#######################
### Core Ontology #####
#######################

dima:Phase a owl:Class ;
    rdfs:label "DIMA Phase"@en ;
    rdfs:label "Phase DIMA"@fr ;
    rdfs:comment "One of the four major stages in the DIMA framework: Detect, Inform, Memorize, Act."@en ;
    rdfs:comment "L'une des quatre phases majeures du cadre DIMA : Détecter, Informer, Mémoriser, Agir."@fr .

dima:Tactic a owl:Class ;
    rdfs:label "Narrative Tactic"@en ;
    rdfs:label "Tactique narrative"@fr ;
    rdfs:comment "A communicative strategy used to exploit cognitive biases in narrative content."@en ;
    rdfs:comment "Une stratégie narrative exploitant des biais cognitifs dans un discours."@fr .

dima:Technique a owl:Class ;
    rdfs:label "Cognitive Technique"@en ;
    rdfs:label "Technique cognitive"@fr ;
    rdfs:comment "A cognitive bias or psychological mechanism leveraged by a tactic."@en ;
    rdfs:comment "Un biais cognitif ou mécanisme psychologique utilisé dans une tactique."@fr .

# Optional: Ensure disjointness
dima:Tactic owl:disjointWith dima:Technique .
dima:Phase owl:disjointWith dima:Tactic .
dima:Phase owl:disjointWith dima:Technique .

##########################
### Object Properties ####
##########################

dima:includesTechnique a owl:ObjectProperty ;
    rdfs:label "includes technique"@en ;
    rdfs:label "inclut la technique"@fr ;
    rdfs:domain dima:Tactic ;
    rdfs:range dima:Technique ;
    rdfs:comment "Links a narrative tactic to one or more cognitive techniques."@en ;
    rdfs:comment "Relie une tactique à une ou plusieurs techniques cognitives."@fr .

dima:isTechniqueOf a owl:ObjectProperty ;
    owl:inverseOf dima:includesTechnique ;
    rdfs:label "is technique of"@en ;
    rdfs:label "est une technique de"@fr ;
    rdfs:domain dima:Technique ;
    rdfs:range dima:Tactic ;
    rdfs:comment "Inverse property linking a technique to its associated tactic(s)."@en ;
    rdfs:comment "Propriété inverse reliant une technique à la ou les tactiques associées."@fr .

dima:belongsToPhase a owl:ObjectProperty ;
    rdfs:label "belongs to phase"@en ;
    rdfs:label "appartient à la phase"@fr ;
    rdfs:domain dima:Tactic ;
    rdfs:range dima:Phase ;
    rdfs:comment "Associates a tactic with a DIMA phase (Detect, Inform, etc.)."@en ;
    rdfs:comment "Associe une tactique à une phase DIMA (Détecter, Informer, etc.)."@fr .
	
dima:hasTactic a owl:ObjectProperty ;
    owl:inverseOf dima:belongsToPhase ;
    rdfs:label "has tactic"@en ;
    rdfs:label "a pour tactique"@fr ;
    rdfs:domain dima:Phase ;
    rdfs:range dima:Tactic ;
    rdfs:comment "Inverse property linking a DIMA phase to its narrative tactics."@en ;
    rdfs:comment "Propriété inverse associant une phase DIMA à ses tactiques narratives."@fr .
	
dima:usesTechnique a owl:ObjectProperty ;
    rdfs:label "uses technique"@en ;
    rdfs:label "utilise la technique"@fr ;
    rdfs:domain dima:Phase ;
    rdfs:range dima:Technique ;
    rdfs:comment "Links a DIMA phase to techniques indirectly used through its tactics."@en ;
    rdfs:comment "Relie une phase DIMA aux techniques cognitives qu'elle mobilise via ses tactiques."@fr .

dima:isUsedInPhase a owl:ObjectProperty ;
    owl:inverseOf dima:usesTechnique ;
    rdfs:label "is used in phase"@en ;
    rdfs:label "est utilisée dans la phase"@fr ;
    rdfs:domain dima:Technique ;
    rdfs:range dima:Phase ;
    rdfs:comment "Indicates the DIMA phase in which a technique is applied via associated tactics."@en ;
    rdfs:comment "Indique la phase DIMA dans laquelle une technique est mobilisée via les tactiques associées."@fr .

############################
### Annotation Properties ##
############################

dima:uuid a owl:AnnotationProperty ;
    rdfs:label "UUID"@en ;
    rdfs:label "UUID"@fr ;
    rdfs:comment "Stable unique identifier from the original taxonomy source."@en ;
    rdfs:comment "Identifiant unique et stable issu de la taxonomie source."@fr .

dima:valueCode a owl:AnnotationProperty ;
    rdfs:label "Value code"@en ;
    rdfs:label "Code de valeur"@fr ;
    rdfs:comment "Code from the source taxonomy (e.g., TA0011, TE0111)."@en ;
    rdfs:comment "Code de la taxonomie source (ex. : TA0011, TE0111)."@fr .


#####################
### DETECT Phase  ###
#####################

dima:Detect a owl:Class ;
    rdfs:subClassOf dima:Phase ;
    rdfs:label "DETECT"@en ;
    rdfs:label "DÉTECTER"@fr .

### DETECT Tactics

dima:PreExistingInformation a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Detect ;
    rdfs:label "Pre-existing information"@en ;
    rdfs:label "Information préexistante"@fr ;
    dima:valueCode "TA0011" ;
    dima:uuid "f7b94d9e-2e5b-4082-9f96-ab704e6e1d95" ;
    rdfs:comment "A tactic that uses pre-existing beliefs or knowledge to attract attention."@en ;
    rdfs:comment "Tactique qui utilise des informations préexistantes ou des croyances établies pour capter l’attention de la cible."@fr ;
    dima:includesTechnique dima:AvailabilityHeuristic, dima:MereExposureEffect .

dima:RepeatedExposureInformation a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Detect ;
    rdfs:label "Repeated exposure information"@en ;
    rdfs:label "Information à exposition répétée"@fr ;
    dima:valueCode "TA0012" ;
    dima:uuid "a2d9c63d-f4d6-4183-b0f7-2169b8c2d8d4" ;
    rdfs:comment "A tactic based on exploiting repetition to improve information detection."@en ;
    rdfs:comment "Tactique qui repose sur l’exploitation de la répétition de l’information en vue de faciliter sa détection par la cible."@fr ;
    dima:includesTechnique dima:FrequencyIllusion, dima:ContextEffect .

dima:DivisiveInformation a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Detect ;
    rdfs:label "Divisive information"@en ;
    rdfs:label "Information clivante"@fr ;
    dima:valueCode "TA0013" ;
    dima:uuid "d8f6b6c4-e9b1-4c16-9d8c-2b3ce6d2f332" ;
    rdfs:comment "A tactic that highlights strange, controversial or surprising content to attract attention."@en ;
    rdfs:comment "Tactique qui favorise les informations clivantes, étranges ou surprenantes pour capter l’attention de la cible."@fr ;
    dima:includesTechnique dima:BizarrenessEffect, dima:NegativityBias .

dima:DeviationFromNorm a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Detect ;
    rdfs:label "Deviation from the norm"@en ;
    rdfs:label "Écart à la norme"@fr ;
    dima:valueCode "TA0014" ;
    dima:uuid "b7f72b4a-d8a9-4613-9d1d-f11e01e6527c" ;
    rdfs:comment "A tactic leveraging the detection of novelty or change in context."@en ;
    rdfs:comment "Tactique qui propose repose sur la capacité à détecter les changements dans notre environnement."@fr ;
    dima:includesTechnique dima:VonRestorffEffect, dima:AnchoringBias, dima:ContrastEffect .

dima:SignificantDetail a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Detect ;
    rdfs:label "Significant detail"@en ;
    rdfs:label "Détail signifiant"@fr ;
    dima:valueCode "TA0015" ;
    dima:uuid "e8d27437-6c1d-4e07-9f33-d8e164bb9d2e" ;
    rdfs:comment "A tactic exploiting the human tendency to focus on isolated details and attribute meaning to them."@en ;
    rdfs:comment "Tactique qui exploite la propension d'une cible à détecter un détail et à lui donner du sens."@fr ;
    dima:includesTechnique dima:DistinctionBias, dima:WeberFechnerLaw .

### DETECT Techniques

dima:AvailabilityHeuristic a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "availability heuristic"@en ;
    rdfs:label "Heuristique de disponibilité"@fr ;
    dima:valueCode "TE0111" ;
    rdfs:comment "Using readily available examples or experiences as the basis for decisions, regardless of their representativeness."@en ;
    rdfs:comment "Décision fondée sur des exemples ou souvenirs facilement accessibles, même s’ils ne sont pas représentatifs."@fr .

dima:MereExposureEffect a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "mere exposure effect"@en ;
    rdfs:label "Effet de simple exposition"@fr ;
    dima:valueCode "TE0112" ;
    rdfs:comment "Tendency to develop preference for things merely because they are familiar."@en ;
    rdfs:comment "Préférence croissante envers ce qui est familier par exposition répétée."@fr .

dima:FrequencyIllusion a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "frequency illusion"@en ;
    rdfs:label "Illusion de la fréquence"@fr ;
    dima:valueCode "TE0121" ;
    rdfs:comment "Perception that something occurs more frequently after first noticing it — also called the Baader-Meinhof effect."@en ;
    rdfs:comment "Croire qu’un phénomène est plus fréquent après l’avoir remarqué une fois (effet Baader-Meinhof)."@fr .

dima:ContextEffect a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "context effect"@en ;
    rdfs:label "Effet de contexte"@fr ;
    dima:valueCode "TE0122" ;
    rdfs:comment "Tendency to interpret perceptions in relation to nearby or similar situations."@en ;
    rdfs:comment "Analyser une perception par rapport à des situations proches ou similaires."@fr .

dima:BizarrenessEffect a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "bizarreness effect"@en ;
    rdfs:label "Effet de bizarrerie"@fr ;
    dima:valueCode "TE0131" ;
    rdfs:comment "Strange or unusual information is more likely to be noticed and remembered."@en ;
    rdfs:comment "Tendance à remarquer et mémoriser plus facilement les informations étranges."@fr .

dima:NegativityBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "negativity bias"@en ;
    rdfs:label "Biais de négativité"@fr ;
    dima:valueCode "TE0132" ;
    rdfs:comment "Giving more attention and weight to negative experiences over positive ones."@en ;
    rdfs:comment "Accorder plus d’attention et de poids aux expériences négatives qu’aux positives."@fr .

dima:VonRestorffEffect a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "von Restorff effect"@en ;
    rdfs:label "Effet von Restorff"@fr ;
    dima:valueCode "TE0141" ;
    rdfs:comment "Unusual or distinctive items are more likely to be remembered (isolation effect)."@en ;
    rdfs:comment "Ce qui se distingue du reste est mieux retenu en mémoire (effet d’isolement)."@fr .

dima:AnchoringBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "anchoring bias"@en ;
    rdfs:label "Biais d’ancrage"@fr ;
    dima:valueCode "TE0142" ;
    rdfs:comment "Over-reliance on the first piece of information received when making decisions."@en ;
    rdfs:comment "Tendance à se fixer sur la première information reçue dans un jugement."@fr .

dima:ContrastEffect a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "contrast effect"@en ;
    rdfs:label "Effet de contraste"@fr ;
    dima:valueCode "TE0143" ;
    rdfs:comment "Perception of an item is influenced by comparison with what came before."@en ;
    rdfs:comment "Modification de la perception par comparaison avec un autre élément observé auparavant."@fr .

dima:DistinctionBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "distinction bias"@en ;
    rdfs:label "Biais de distinction"@fr ;
    dima:valueCode "TE0151" ;
    rdfs:comment "Tendency to overstate differences between options when evaluated simultaneously."@en ;
    rdfs:comment "Surestimer les différences entre deux options lorsqu’elles sont comparées côte à côte."@fr .

dima:WeberFechnerLaw a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "Weber-Fechner law"@en ;
    rdfs:label "Loi de Weber-Fechner"@fr ;
    dima:valueCode "TE0152" ;
    rdfs:comment "Perception of change depends on relative—not absolute—stimulus intensity."@en ;
    rdfs:comment "Plus un stimulus est intense, plus il faut de changement pour percevoir une différence."@fr .

#####################
### INFORM Phase  ###
#####################

dima:Inform a owl:Class ;
    rdfs:subClassOf dima:Phase ;
    rdfs:label "INFORM"@en ;
    rdfs:label "INFORMER"@fr .

### INFORM Tactics

dima:PatternCreation a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Inform ;
    rdfs:label "Pattern creation"@en ;
    rdfs:label "Création d’un motif"@fr ;
    rdfs:comment "A tactic that integrates received information into an existing narrative."@en ;
    rdfs:comment "Tactique qui permet de mettre en récit une information reçue, de l’intégrer dans un contexte préexistant."@fr ;
    dima:valueCode "TA0021" ;
    dima:includesTechnique dima:IllusoryCorrelationBias,
                           dima:AnecdotalEvidenceBias,
                           dima:GamblersFallacy .

dima:GeneralizationAndStereotypes a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Inform ;
    rdfs:label "Generalization and reinforcement of stereotypes"@en ;
    rdfs:label "Généralisation et renforcement de stéréotypes"@fr ;
    rdfs:comment "A tactic aiming to simplify perceived information to make it more acceptable or understandable to the target."@en ;
    rdfs:comment "Tactique qui vise à simplifier une ou des informations perçues pour les rendre acceptables et/ou intelligibles par la cible."@fr ;
    dima:valueCode "TA0022" ;
    dima:includesTechnique dima:BackfireEffect .

dima:FamiliaritySuperiority a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Inform ;
    rdfs:label "Familiarity superiority"@en ;
    rdfs:label "Supériorité familière"@fr ;
    rdfs:comment "A tactic that enhances the perceived value of information through familiarity or proximity."@en ;
    rdfs:comment "Tactique qui permet de valoriser une information aux yeux de la cible par exploitation des proximités."@fr ;
    dima:valueCode "TA0023" ;
    dima:includesTechnique dima:HomogeneityBias,
                           dima:WellTraveledRoadBias .

dima:Simplification a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Inform ;
    rdfs:label "Simplification"@en ;
    rdfs:label "Simplication"@fr ;
    rdfs:comment "A tactic that simplifies concepts and emphasizes scale to improve information acceptability."@en ;
    rdfs:comment "Tactique qui simplifie les concepts et favorise les ordres de grandeurs pour faciliter l'acceptabilité d'une information."@fr ;
    dima:valueCode "TA0024" ;
    dima:includesTechnique dima:ZeroSumBias,
                           dima:NormalcyBias .

dima:SelfReference a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Inform ;
    rdfs:label "Self-reference"@en ;
    rdfs:label "Auto-référence"@fr ;
    rdfs:comment "A tactic based on the assumption that the audience shares the same knowledge and references."@en ;
    rdfs:comment "Tactique qui exploite le fait que la cible pense que ses interlocuteurs partagent ses références communes."@fr ;
    dima:valueCode "TA0025" ;
    dima:includesTechnique dima:FalseConsensusEffect .

dima:TemporalProjection a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Inform ;
    rdfs:label "Temporal projection"@en ;
    rdfs:label "Projection temporelle"@fr ;
    rdfs:comment "A tactic that draws on (real or fabricated) memories to legitimize a message."@en ;
    rdfs:comment "Tactique qui s’appuie sur les souvenirs (réels ou forgés) pour crédibiliser un discours ou une information."@fr ;
    dima:valueCode "TA0026" ;
    dima:includesTechnique dima:HindsightBias .

### INFORM Techniques

dima:IllusoryCorrelationBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "illusory correlation bias"@en ;
    rdfs:label "Biais de corrélation illusoire"@fr ;
    rdfs:comment "Perceiving a false link between unrelated events or data."@en ;
    rdfs:comment "Tendance à percevoir un lien entre des événements ou des éléments non corrélés."@fr ;
    dima:valueCode "TE0211" .

dima:AnecdotalEvidenceBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "anecdotal evidence bias"@en ;
    rdfs:label "Biais de la preuve anecdotique"@fr ;
    rdfs:comment "Giving more weight to vivid personal stories than to statistical evidence."@en ;
    rdfs:comment "Accorder plus de poids aux récits personnels qu'aux preuves statistiques."@fr ;
    dima:valueCode "TE0212" .

dima:GamblersFallacy a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "gambler’s fallacy"@en ;
    rdfs:label "Illusion des séries"@fr ;
    rdfs:comment "Believing that past random events influence future ones."@en ;
    rdfs:comment "Croire que les événements passés influencent la probabilité des événements futurs."@fr ;
    dima:valueCode "TE0213" .

dima:BackfireEffect a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "backfire effect"@en ;
    rdfs:label "Effet de retournement"@fr ;
    rdfs:comment "Reinforcing one’s beliefs when confronted with contradictory information."@en ;
    rdfs:comment "Tendance à renforcer ses croyances lorsqu'on est confronté à des preuves contraires."@fr ;
    dima:valueCode "TE0221" .

dima:HomogeneityBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "homogeneity bias"@en ;
    rdfs:label "Biais d'homogénéité"@fr ;
    rdfs:comment "Perceiving members of an out-group as more similar than they really are."@en ;
    rdfs:comment "Tendance à voir les membres d’un groupe externe comme homogènes."@fr ;
    dima:valueCode "TE0231" .

dima:WellTraveledRoadBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "well-traveled road bias"@en ;
    rdfs:label "Biais de la route connue"@fr ;
    rdfs:comment "Preferring familiar paths even when others are better."@en ;
    rdfs:comment "Préférer ce qui est familier, même si d'autres options sont meilleures."@fr ;
    dima:valueCode "TE0232" .

dima:ZeroSumBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "zero-sum bias"@en ;
    rdfs:label "Biais de la somme nulle"@fr ;
    rdfs:comment "Believing that gains for others mean losses for oneself."@en ;
    rdfs:comment "Croire que le gain de l’un implique la perte de l’autre."@fr ;
    dima:valueCode "TE0241" .

dima:NormalcyBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "normalcy bias"@en ;
    rdfs:label "Biais de normalité"@fr ;
    rdfs:comment "Underestimating the likelihood of a crisis because things have always been normal."@en ;
    rdfs:comment "Penser que rien ne changera parce que les choses ont toujours été normales."@fr ;
    dima:valueCode "TE0242" .

dima:FalseConsensusEffect a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "false consensus effect"@en ;
    rdfs:label "Effet de faux consensus"@fr ;
    rdfs:comment "Overestimating how much others share your views or behaviors."@en ;
    rdfs:comment "Surestimer à quel point les autres partagent nos opinions ou comportements."@fr ;
    dima:valueCode "TE00251" .

dima:HindsightBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "hindsight bias"@en ;
    rdfs:label "Biais rétrospectif"@fr ;
    rdfs:comment "Believing events were more predictable after they happened."@en ;
    rdfs:comment "Croire qu’un événement était prévisible après qu’il se soit produit."@fr ;
    dima:valueCode "TE0261" .

######################
### MEMORISE Phase ###
######################

dima:Memorise a owl:Class ;
    rdfs:subClassOf dima:Phase ;
    rdfs:label "MEMORISE"@en ;
    rdfs:label "MÉMORISER"@fr .

### MEMORISE Tactics

dima:IndirectReinforcement a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Memorise ;
    rdfs:label "Indirect reinforcement"@en ;
    rdfs:label "Renforcement indirect"@fr ;
    dima:valueCode "TA0031" ;
    rdfs:comment "A tactic that supports memory formation by reinforcing secondary sources."@en ;
    rdfs:comment "Tactique qui favorise la mémorisation et la construction de souvenirs par renforcement de sources secondaires."@fr ;
    dima:includesTechnique dima:SourceConfusionBias, dima:SpacingEffect, dima:SuggestionEffect .

dima:PreexistingReinforcement a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Memorise ;
    rdfs:label "Pre-existing reinforcement"@en ;
    rdfs:label "Renforcement pré-existant"@fr ;
    dima:valueCode "TA0032" ;
    rdfs:comment "A tactic that helps memorize information by leveraging stereotypes or pre-existing beliefs."@en ;
    rdfs:comment "Tactique qui favorise la mémorisation d'une information en s'appuyant sur des stéréotypes ou des croyances pré-existantes."@fr ;
    dima:includesTechnique dima:ImplicitStereotype, dima:FadingAffectBias, dima:ContextEffect_Memorise .

dima:ContentExposure a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Memorise ;
    rdfs:label "Content exposure"@en ;
    rdfs:label "Exposition de contenus"@fr ;
    dima:valueCode "TA0033" ;
    rdfs:comment "A tactic that promotes memorization through overexposure to information."@en ;
    rdfs:comment "Tactique qui favorise la surexposition à une information pour favoriser sa mémorisation."@fr ;
    dima:includesTechnique dima:RecencyEffect, dima:MereExposureEffect_Memorise, dima:PrimacyEffect .

### MEMORISE Techniques

dima:SourceConfusionBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "source confusion bias"@en ;
    rdfs:label "biais de confusion de source"@fr ;
    dima:valueCode "TE0312" ;
    rdfs:comment "Confusing the source of a memory, attributing it incorrectly."@en ;
    rdfs:comment "Confusion entre la source d'un souvenir réel et une information extérieure."@fr .

dima:SpacingEffect a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "spacing effect"@en ;
    rdfs:label "effet d'espacement"@fr ;
    dima:valueCode "TE0313" ;
    rdfs:comment "Improved memory retention when learning is spaced over time."@en ;
    rdfs:comment "Mémorisation renforcée grâce à une exposition répétée avec des intervalles de temps."@fr .

dima:SuggestionEffect a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "suggestion effect"@en ;
    rdfs:label "Effet de suggestion"@fr ;
    dima:valueCode "TE0314" ;
    rdfs:comment "The tendency for suggestions to influence memory construction."@en ;
    rdfs:comment "Tendance à altérer ou créer des souvenirs sous l'effet de suggestions extérieures."@fr .

dima:ImplicitStereotype a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "implicit stereotype"@en ;
    rdfs:label "Stéréotype implicite"@fr ;
    dima:valueCode "TE0321" ;
    rdfs:comment "Stereotypes that influence judgment or memory unconsciously."@en ;
    rdfs:comment "Stéréotypes influençant inconsciemment la mémoire ou le jugement."@fr .

dima:FadingAffectBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "fading affect bias"@en ;
    rdfs:label "Effacement négatif"@fr ;
    dima:valueCode "TE0322" ;
    rdfs:comment "Tendency to forget negative emotions faster than positive ones."@en ;
    rdfs:comment "Les émotions négatives s'effacent plus vite que les positives dans les souvenirs."@fr .

dima:ContextEffect_Memorise a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "context effect"@en ;
    rdfs:label "Effet de contexte"@fr ;
    dima:valueCode "TE0323" ;
    rdfs:comment "Memory is influenced by environmental or emotional context."@en ;
    rdfs:comment "La mémorisation est affectée par le contexte environnemental ou émotionnel."@fr .

dima:RecencyEffect a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "recency effect"@en ;
    rdfs:label "Effet de récence"@fr ;
    dima:valueCode "TE0331" ;
    rdfs:comment "Most recent information is remembered better."@en ;
    rdfs:comment "Les informations les plus récentes influencent davantage la mémoire et les décisions."@fr .

dima:MereExposureEffect_Memorise a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "mere exposure effect"@en ;
    rdfs:label "Effet de simple exposition"@fr ;
    dima:valueCode "TE0332" ;
    rdfs:comment "Repeated exposure to a stimulus increases familiarity and preference."@en ;
    rdfs:comment "La répétition d'exposition à une information renforce sa mémorisation et son acceptabilité."@fr .

dima:PrimacyEffect a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "primacy effect"@en ;
    rdfs:label "Effet de primauté"@fr ;
    dima:valueCode "TE0333" ;
    rdfs:comment "First information encountered is remembered more easily and influences perception."@en ;
    rdfs:comment "Les premières informations reçues ont un impact durable sur la perception et la mémoire."@fr .

##################
### ACT Phase  ###
##################

dima:Act a owl:Class ;
    rdfs:subClassOf dima:Phase ;
    rdfs:label "ACT"@en ;
    rdfs:label "AGIR"@fr .

### ACT Tactics

dima:IndividualValorisation a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Act ;
    rdfs:label "Individual valorisation"@en ;
    rdfs:label "Valorisation individuelle"@fr ;
    dima:valueCode "TA0041" ;
    rdfs:comment "A tactic that emphasizes personal judgment and self-confidence."@en ;
    rdfs:comment "Tactique qui valorise le jugement personnel et la confiance en soi."@fr ;
    dima:includesTechnique dima:OverconfidenceBias, dima:PeltzmanEffect, dima:IllusorySuperiority .

dima:EscalationReinforcement a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Act ;
    rdfs:label "Escalation reinforcement"@en ;
    rdfs:label "Renforcement escalatoire"@fr ;
    dima:valueCode "TA0042" ;
    rdfs:comment "A tactic that encourages continuation of actions already initiated."@en ;
    rdfs:comment "Tactique qui favorise la réalisation d'actions déjà engagées."@fr ;
    dima:includesTechnique dima:SunkCostBias, dima:AuthorityBias .

dima:OzaekomiWaza a owl:Class ;
    rdfs:subClassOf dima:Tactic ;
    dima:belongsToPhase dima:Act ;
    rdfs:label "Ozaekomi waza (immobilization control)"@en ;
    rdfs:label "Ozaekomi waza (contrôle par immobilisation)"@fr ;
    dima:valueCode "TA0043" ;
    rdfs:comment "A tactic that induces passivity or decision paralysis in the target."@en ;
    rdfs:comment "Tactique qui consiste à favoriser chez une cible, l'immobilisme et l'absence d'action."@fr ;
    dima:includesTechnique dima:OmissionBias, dima:StatusQuoBias, dima:InformationOverload .

### ACT Techniques

dima:OverconfidenceBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "overconfidence bias"@en ;
    rdfs:label "biais d'excès de confiance"@fr ;
    dima:valueCode "TE0411" ;
    rdfs:comment "Overestimating one's abilities, performance, or judgment."@en ;
    rdfs:comment "Surestimer ses compétences, sa performance ou son jugement."@fr .

dima:PeltzmanEffect a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "Peltzman effect"@en ;
    rdfs:label "Effet Peltzman (compensation du risque)"@fr ;
    dima:valueCode "TE0412" ;
    rdfs:comment "Tendency to take greater risks when one feels protected."@en ;
    rdfs:comment "Tendance à prendre plus de risques en se sentant protégé."@fr .

dima:IllusorySuperiority a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "illusory superiority"@en ;
    rdfs:label "Supériorité illusoire"@fr ;
    dima:valueCode "TE0413" ;
    rdfs:comment "Belief that one is better than others, often unrealistically."@en ;
    rdfs:comment "Croyance que l’on est meilleur que les autres, souvent à tort."@fr .

dima:SunkCostBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "sunk cost bias"@en ;
    rdfs:label "biais des coûts irrécupérables"@fr ;
    dima:valueCode "TE0421" ;
    rdfs:comment "Tendency to continue an endeavor once an investment in money, effort, or time has been made."@en ;
    rdfs:comment "Tendance à poursuivre un effort déjà engagé en raison d’un investissement passé."@fr .

dima:AuthorityBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "authority bias"@en ;
    rdfs:label "biais d'autorité"@fr ;
    dima:valueCode "TE0422" ;
    rdfs:comment "Tendency to follow directives or believe information from perceived authorities."@en ;
    rdfs:comment "Tendance à accorder plus de crédibilité ou à suivre les directives de figures d’autorité."@fr .

dima:OmissionBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "omission bias"@en ;
    rdfs:label "Biais d'omission"@fr ;
    dima:valueCode "TE0431" ;
    rdfs:comment "Preferring inaction over action even when it leads to worse outcomes."@en ;
    rdfs:comment "Considérer qu'un tort causé par inaction est moins grave qu’un tort causé par action."@fr .

dima:StatusQuoBias a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "status quo bias"@en ;
    rdfs:label "Biais du statu quo"@fr ;
    dima:valueCode "TE0432" ;
    rdfs:comment "Preference for keeping things as they are, avoiding change."@en ;
    rdfs:comment "Préférence pour le statu quo même si d'autres options sont meilleures."@fr .

dima:InformationOverload a owl:Class ;
    rdfs:subClassOf dima:Technique ;
    rdfs:label "information overload"@en ;
    rdfs:label "Saturation informationnelle"@fr ;
    dima:valueCode "TE0433" ;
    rdfs:comment "Overloading a target with information to disrupt decision-making (analysis paralysis)."@en ;
    rdfs:comment "Saturation de la cible d’informations afin de paralyser la prise de décision."@fr .
